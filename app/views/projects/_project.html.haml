- last_build = project.last_build
%tr.alert{class: project_statuc_class(project) }
  %td
    = project.id
  %td
    = link_to project do
      %strong= project.name
  %td
    - if last_build
      #{last_build.status} (#{build_or_build_group_link(last_build)})
      = time_ago_in_words project.last_build_date
      ago
    - else
      No builds yet
  %td
    - if project.public
      %i.icon-globe
      Public
    - else
      %i.icon-lock
      Private
  %td
    = project.builds.count
